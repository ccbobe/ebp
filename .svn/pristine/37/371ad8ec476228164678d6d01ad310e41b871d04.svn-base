package com.oracle.ebp.util;

import java.io.Serializable;

public class Perchase implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 2L;
	private Integer tid;
	private Integer buyNums;
	private Double price;
	private Double amount; 
	private String descs;
	public Integer getTid() {
		return tid;
	}
	public void setTid(Integer tid) {
		this.tid = tid;
	}
	public Integer getBuyNums() {
		return buyNums;
	}
	public void setBuyNums(Integer buyNums) {
		this.buyNums = buyNums;
	}
	public Double getPrice() {
		return price;
	}
	public void setPrice(Double price) {
		this.price = price;
	}

	public Double getAmount() {
		return amount;
	}
	public void setAmount(Double amount) {
		this.amount = amount;
	}
	
	public String getdescs() {
		return descs;
	}
	public void setdescs(String descs) {
		this.descs = descs;
	}

	@Override
	public String toString() {
		return "Perchase [tid=" + tid + ", buyNums=" + buyNums + ", price="
				+ price + ", amount=" + amount + ", descs=" + descs + "]";
	}
	public Perchase(Integer tid, Integer buyNums, Double price ,Double amount , String descs) {
		super();
		this.tid = tid;
		this.buyNums = buyNums;
		this.price = price;
		this.amount = amount;
		this.descs = descs;
	}
	public Perchase() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	

}
